/*下拉菜单组件*/
.dropdown-menu {
    border-color: #dedede;
}
.dropdown-header,
.dropdown-menu > li > a,
.dropdown-menu > li > .title {
    padding: 4px 20px;
}
.dropdown-menu > li > .title{
    display: block;
}
.dropdown-header{font-size: 1em; font-size: inherit;}
.dropdown-menu > li.focus > a,
.dropdown-menu > li.focus > .title,
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus {
  background-color: #e8e8e8;
}
.dropdown-menu > .active > a,
.dropdown-menu > .active > a:hover,
.dropdown-menu > .active > a:focus {
  background-color: #2e6da4;
}
.dropdown-menu .divider {
    margin: 6px 0;
}

.dropdown-toggle ~ .dropdown-menu li.selected a{
    background: #e8e8e8;
}

/*导航组件*/
.navbar-transparent {
    background: transparent;
    border-bottom-color: rgba(0, 0, 0, .1);
}
.navbar-transparent .navbar-collapse{
    border-top-color: rgba(0, 0, 0, .1);
}
.navbar-transparent .navbar-toggle,
.navbar-transparent .navbar-nav > li > a {
    color: #fff;
    filter:alpha(opacity=90); /* IE */  
    -moz-opacity:0.9; /* 老版Mozilla */  
    -khtml-opacity:0.9; /* 老版Safari */  
    opacity: 0.9; /* 支持opacity的浏览器*/
}
.navbar-transparent .nav > li > a:hover,
.navbar-transparent .nav .open>a,
.navbar-transparent .nav .open>a:focus, 
.navbar-transparent .nav .open>a:hover,
.navbar-transparent .nav > .active > a {
    background-color: rgba(0, 0, 0, .1);
    filter: alpha(opacity=100); /* IE */
    -moz-opacity: 1.0; /* 老版Mozilla */  
    -khtml-opacity: 1.0; /* 老版Safari */  
    opacity: 1.0; /* 支持opacity的浏览器*/
}

.navbar-transparent .navbar-toggle {
    background-color: rgba(0, 0, 0, .1);
}
.navbar-transparent .navbar-toggle .icon-bar {
    background-color: #fafafa;
}

.navbar-default,
.navbar-transparent.affix{
  background-image: -webkit-linear-gradient(top, #fff 0%, #f8f8f8 100%);
  background-image:      -o-linear-gradient(top, #fff 0%, #f8f8f8 100%);
  background-image: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#f8f8f8));
  background-image:         linear-gradient(to bottom, #fff 0%, #f8f8f8 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffffff', endColorstr='#fff8f8f8', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
  background-repeat: repeat-x;
  -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 5px rgba(0, 0, 0, .075);
          box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 5px rgba(0, 0, 0, .075);
}
