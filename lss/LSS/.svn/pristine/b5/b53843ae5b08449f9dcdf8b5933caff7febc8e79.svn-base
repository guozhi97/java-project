    .form-inline .form-group .col-md-2,
    .form-horizontal .form-group .col-sm-2,
    .form-horizontal .form-group .col-md-2{ max-width: 144px;}

    .form-inline .form-group .col-sm-2 ~ .col-sm-10,
    .form-horizontal .form-group .col-sm-2 ~ .col-sm-10{ padding-left: 0;}
    
    /*重置水平表单或内联表单中输入框标签的内间距的值*/
    .form-group-sm .control-label{ padding-top: 4px !important; vertical-align: baseline;}

    .dl-horizontal dt{ width: 144px;}
    .dl-horizontal dd{ margin-left: 164px;}
}
/* bootstrap css 改写结束 */