.navbar-static-top,
.navbar-fixed-top,
.navbar-fixed-bottom {
    border-radius: 0;
    z-index: 990;
}

.navbar-brand {
    font-size: 0;
    min-width: 156px;
    background: url('../../images/logo_qisharp.png') no-repeat left center;
